logfile = open('test.log', 'w')
i = 0
while (i < 100):
    i += 1
    toWrite = "Number: " + str(i) + "\n"
    logfile.write(toWrite)

logfile.close()

logfile = open('test.log')

#This last little bit is 
done = False
while (done == False):
    line = logfile.readline()
    if (line != ''):
        print line
    else:
        done = True

    

    

